{
  "name": "ember-svg-jar",
  "version": "2.3.4",
  "description": "Best way to use SVG images in Ember applications",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "Ivan Votti <ivanvotti@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "homepage": "https://svgjar.web.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/ivanvotti/ember-svg-jar.git"
  },
  "bugs": {
    "url": "https://github.com/ivanvotti/ember-svg-jar/issues"
  },
  "scripts": {
    "lerna:lint-staged": "lint-staged",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:js:fix": "eslint . --fix",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:node": "mocha node-tests/**/*-test.js",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "broccoli-caching-writer": "^3.0.3",
    "broccoli-concat": "^4.2.5",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-persistent-filter": "^3.1.2",
    "broccoli-plugin": "^4.0.7",
    "broccoli-string-replace": "^0.1.2",
    "broccoli-svg-optimizer": "2.0.0",
    "cheerio": "^0.22.0",
    "console-ui": "^3.1.1",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.7.1",
    "lodash": "^4.17.15",
    "safe-stable-stringify": "^2.2.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^0.50.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "all-contributors-cli": "^6.9.1",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-fixture": "^1.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "ember-auto-import": "^2.2.0",
    "ember-cli": "~3.28.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-fastboot": "^2.2.3",
    "ember-cli-fastboot-testing": "^0.6.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.28.8",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.0.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "lint-staged": ">=10",
    "loader.js": "^4.7.0",
    "mocha": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^2.0.0",
    "webpack": "5"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "versionCompatibility": {
      "ember": ">=1.13.13"
    }
  },
  "lint-staged": {
    "**/*.{json,html,yml,css,scss,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix"
    ]
  }
}
